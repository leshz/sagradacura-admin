{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
      "builder": "NIXPACKS",
      "nixpacksPlan": {
        "providers": [
          "..."
        ]
      }
    },
    "deploy": {
      "runtime": "V2",
      "numReplicas": 1,
      "healthcheckPath": "/",
      "sleepApplication": true,
      "restartPolicyType": "NEVER",
      "healthcheckTimeout": 60
    }
  }